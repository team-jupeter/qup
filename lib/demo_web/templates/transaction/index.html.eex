<table>
  <thead>
    <tr>
      <th>seller</th>
      <th>amount</th>
      <th>archived?</th>
    </tr>
  </thead>
  <tbody>
<%= for transaction <- @transactions do %>
    <tr> 
      <td><%= transaction.seller_name %></td>
      <td><%= transaction.abc_amount %></td>
      <td><%= transaction.archived? %></td>
    </tr>
      <td>
        <span><%= link "Show", to: Routes.transaction_path(@conn, :show, transaction) %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

