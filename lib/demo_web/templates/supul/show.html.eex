<h4><%= @supul.name %></h4>
<ul>
  <li>
    <strong>Type:</strong>
    <%= @supul.type %>
  </li>
  
  <li>
    <strong>Supul Code:</strong>
    <%= @supul.supul_code %>
  </li>
</ul>


