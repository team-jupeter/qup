<h3>Show Nation</h3>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @nation.name %>
  </li>

  <li>
    <strong>ID:</strong>
    <%= @nation.id %>
  </li>

  <li>
    <strong>Constitution Signature:</strong>
    <%= @nation.signature %>
