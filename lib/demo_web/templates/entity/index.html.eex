<table>
  <%= for entity <- @entities do %>
    <tr>
      <td><%= render "entity.html", entity: entity %></td>
      <td><%= link "View", to: Routes.entity_path(@conn, :show, entity.id) %></td>
    </tr>
  <% end %>

</table>
  <span><%= link "New Entity", to: Routes.entity_path(@conn, :new) %></span>

</br>
<span><%= link "User Home", to: Routes.user_path(@conn, :index) %></span>
